{
  "name": "sortable-map",
  "version": "0.0.17",
  "description": "Dictionary data structure with flexible sort capabilities",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "lint": "eslint src -f html --ext .js -o lint-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/sortable-map.git"
  },
  "keywords": [
    "javascript",
    "dictionary",
    "sortable",
    "map",
    "sortable",
    "map"
  ],
  "author": "Mike Chabot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechabot/sortable-map/issues"
  },
  "homepage": "https://github.com/mikechabot/sortable-map#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1"
  }
}
